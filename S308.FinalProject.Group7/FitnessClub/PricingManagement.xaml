<Window x:Name="winPricingManagement" x:Class="FitnessClub.PricingManagement"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitnessClub"
        mc:Ignorable="d"
        Title="Pricing Management" Height="649" Width="1200" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image x:Name="imgBackground" Source="Gym Background.jpg" Stretch="Fill" d:IsLocked="True"/>
        <Label x:Name="label_Copy" Content="PRICING MANAGEMENT" HorizontalAlignment="Center" Margin="34,31,774,0" VerticalAlignment="Top" Height="52" Width="386" Foreground="#FFC3B253" FontSize="32" FontFamily="Century Gothic" FontWeight="Bold"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="253" Padding="10" Margin="34,292,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="446" Background="{x:Null}" BorderBrush="White" BorderThickness="5" Foreground="White" FontFamily="Century Gothic" FontSize="24"/>
        <ComboBox x:Name="cmbMembershipType" HorizontalAlignment="Left" Margin="289,115,0,0" VerticalAlignment="Top" Width="213" Height="36" Background="White" FontFamily="Century Gothic"/>
        <Label x:Name="lblLastName" Content="Membership Type:" HorizontalAlignment="Left" Margin="34,112,0,0" VerticalAlignment="Top" FontSize="24" Foreground="White" FontFamily="Century Gothic"/>
        <Button x:Name="btnMainMenu" Content="MAIN MENU" HorizontalAlignment="Left" Margin="970,31,0,0" VerticalAlignment="Top" Width="183" Background="Black" Foreground="White" BorderBrush="White" BorderThickness="5" Height="50" FontSize="26.667" FontWeight="Bold"/>
        <Label x:Name="lblNewPrice" Content="New Price:" HorizontalAlignment="Left" Margin="662,292,0,0" VerticalAlignment="Top" FontSize="18" Foreground="White" FontFamily="Century Gothic"/>
        <Button x:Name="btnTypeCheck" Content="CHECK TYPE" HorizontalAlignment="Left" Margin="166,198,0,0" VerticalAlignment="Top" Width="182" Background="Black" Foreground="White" BorderBrush="White" BorderThickness="5" Height="50" FontSize="26.667" FontWeight="Bold"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="36" Margin="813,292,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="213" FontSize="29.333"/>

    </Grid>
</Window>
