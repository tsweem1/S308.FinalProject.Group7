<Window x:Name="winMembershipSales" x:Class="FitnessClub.MembershipSales"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitnessClub"
        mc:Ignorable="d"
        Title="Membership Sales" Height="649
        " Width="1200">
    <Grid>
        <Image x:Name="Gym_Background_jpg" Source="Gym Background.jpg" Stretch="Fill" Grid.ColumnSpan="2"/>
        <Grid HorizontalAlignment="Left" Height="267" Margin="415,341,0,0" VerticalAlignment="Top" Width="755" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="0*"/>
            </Grid.ColumnDefinitions>
        </Grid>
        <ComboBox x:Name="cboMembershipType" HorizontalAlignment="Left" Margin="366,116,0,0" VerticalAlignment="Top" Width="205" Height="43" RenderTransformOrigin="0.743,0.489" FontFamily="Arial" FontSize="18.667" Background="White" TabIndex="0" Grid.Column="1"/>
        <Label x:Name="lblInstructions" Content="Select Membership Type:" HorizontalAlignment="Left" Margin="21,114,0,0" VerticalAlignment="Top" Height="50" Width="333" FontFamily="Century Gothic" Foreground="#FFF0E9E9" FontSize="26.667" FontWeight="Bold" RenderTransformOrigin="0.5,0.5" Grid.Column="1">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.158"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.747"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="lblTitle" Content="MEMBERSHIP SALES" HorizontalAlignment="Left" Height="62" VerticalAlignment="Top" Width="489" Foreground="#FFC3B253" FontFamily="Century Gothic" FontSize="48" Margin="20,10,0,0" FontWeight="Bold" Grid.Column="1"/>
        <TextBox x:Name="txtEndDate" HorizontalAlignment="Left" Height="51" Margin="293,285,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="216" FontFamily="Arial" FontSize="18.667" TabIndex="3" Grid.Column="1"/>
        <TextBox x:Name="txtStartDate" HorizontalAlignment="Left" Height="51" Margin="20,285,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="216" FontFamily="Arial" FontSize="18.667" TabIndex="2" Grid.Column="1"/>
        <Label x:Name="lblStartDate" Content="Enter Start Date:" HorizontalAlignment="Left" Height="37" Margin="20,243,0,0" VerticalAlignment="Top" Width="213" Foreground="#FFF7F2F2" FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Grid.Column="1"/>
        <Label x:Name="lblEndDate" Content="Enter End Date:" HorizontalAlignment="Left" Height="37" Margin="293,243,0,0" VerticalAlignment="Top" Width="231" Foreground="#FFF7F2F2" FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Grid.Column="1"/>
        <Label x:Name="lblTo" Content="to" HorizontalAlignment="Left" Height="37" Margin="248,285,0,0" VerticalAlignment="Top" Width="40" Foreground="#FFF7F2F2" FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Grid.Column="1"/>
        <Label x:Name="lblMembershipFeatures" Content="Select Membership Features:" HorizontalAlignment="Left" Margin="16,370,0,0" VerticalAlignment="Top" Height="40" Width="388" FontFamily="Century Gothic" Foreground="White" FontSize="26.667" FontWeight="Bold" RenderTransformOrigin="0.5,0.5" Grid.Column="1">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.158"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.747"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <ComboBox x:Name="cboMembershipFeatures" HorizontalAlignment="Left" Margin="20,422,0,0" VerticalAlignment="Top" Width="548" Height="44" RenderTransformOrigin="0.743,0.489" FontFamily="Arial" FontSize="18.667" Background="White" TabIndex="4" Grid.Column="1"></ComboBox>
        <Button x:Name="btnAdd" Content="ADD" HorizontalAlignment="Left" Margin="20,512,0,0" VerticalAlignment="Top" Width="128" FontFamily="Century Gothic" FontWeight="Bold" FontSize="26.667" Height="54" Background="Black" Foreground="#FFFFFBFB" BorderThickness="5" BorderBrush="White" TabIndex="5" Grid.Column="1"/>
        <Button x:Name="btnRemove" Content="REMOVE" HorizontalAlignment="Left" Margin="186,512,0,0" VerticalAlignment="Top" Width="128" FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Height="54" Background="Black" Foreground="#FFFFFBFB" BorderThickness="5" BorderBrush="White" TabIndex="6" Grid.Column="1"/>
        <TextBox x:Name="txtMemberQuotaOutput" HorizontalAlignment="Left" Height="303" Margin="630,178,0,0" Padding="10" TextWrapping="Wrap" VerticalAlignment="Top" Width="524" FontFamily="Arial" Background="{x:Null}" BorderBrush="White" BorderThickness="5" Foreground="White" FontSize="18.667" Grid.Column="1"/>
        <Label x:Name="lblQuotaEstimate" Content="Quota Estimate" HorizontalAlignment="Left" Margin="634,113,0,0" VerticalAlignment="Top" Height="49" Width="231" FontFamily="Century Gothic" Foreground="White" FontSize="26.667" RenderTransformOrigin="0.519,1.403" FontWeight="Bold" Grid.Column="1">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.158"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.759"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Button x:Name="btnMainMenu1" Content="MAIN MENU" HorizontalAlignment="Left" Margin="982,29,0,0" VerticalAlignment="Top" Width="170" FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Height="54" Background="Black" Foreground="#FFFFFBFB" BorderThickness="5" BorderBrush="White" Grid.Column="1"/>
        <Button x:Name="btnRegistration" Content="PROCEED TO REGISTRATION" HorizontalAlignment="Left" Margin="778,515,0,0" VerticalAlignment="Top" Width="374" FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Height="54" Background="Black" Foreground="#FFFFFBFB" BorderThickness="5" BorderBrush="White" Grid.Column="1"/>
        <Button x:Name="btnClear" Content="CLEAR" HorizontalAlignment="Left" Margin="358,512,0,0" VerticalAlignment="Top" Width="128" FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Height="54" Background="Black" Foreground="#FFFFFBFB" BorderThickness="5" BorderBrush="White" TabIndex="7" Grid.Column="1"/>
        <Label x:Name="lblPrice" Content="Price:" HorizontalAlignment="Left" Height="37" Margin="20,182,0,0" VerticalAlignment="Top" Width="213" Foreground="#FFF7F2F2" FontFamily="Century Gothic" FontSize="26.667" FontWeight="Bold" Grid.Column="1"/>
        <TextBox x:Name="txtPrice" HorizontalAlignment="Left" Height="37" Margin="117,182,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="197" FontFamily="Arial" FontSize="18.667" TabIndex="1" Grid.Column="1"/>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="295*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
    </Grid>
</Window>
